<% if @room.photos.count > 0 %>
  <br/><br/>

  <div class="row">
    <% @room.photos.each do |photo| %>
      <div class="col-md-4">
      <div class="panel panel-default">
        <div class="card">
          <%= cl_image_tag photo.key, crop: :fill  %>
        </div>
      </div>
    </div>
    <% end %>
  </div>
<% end %>
