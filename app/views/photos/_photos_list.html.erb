<%  @room.photos.count > 0 %>
  <br/><br/>

  <div class="row">
    <% @room.photos.each do |photo| %>
      <div class="col-md-4">
      <div class="panel panel-default">
        <div class="card">
          <%= cl_image_tag photo.key, crop: :fill  %>
        </div>
          <div class="panel-body">
            <span class="pull-right">
              <%# <%= link_to   room_photo_path(photo.room_id, photo), remote: true,  method: :delete %> %>
                <i class="fa-solid fa-trash-can"></i>
          <% end %>
        </span>
      </div>
    </div>
  </div>
