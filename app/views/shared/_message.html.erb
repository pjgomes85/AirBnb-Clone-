<% if notice %>
            <p class="alert alert-success alert-dismissible col-md-2" id="one">
              <%= notice  %><button type="button" class="close" data-dismiss="alert">&times;</button>
            <% end %>
            </p>

       <p class="alert"><%= alert %></p>

<script>
var alert = document.getElementById("one");
setTimeout(function() {
  alert.style.display = 'none';
}, 10000)
</script>
